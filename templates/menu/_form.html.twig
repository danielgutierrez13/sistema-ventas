{% import '@App/theme1/form.html.twig' as frm %}
{#{% import 'macro/action.html.twig' as frm %}#}

{{ frm.frm_start(form) }}
    {{ frm.frm_column(
        frm.frm_column_field_select(form.parent),
        null,
        null,
        null,
        2
    ) }}
    {{ frm.frm_column(
        frm.frm_column_field(form.name),
        frm.frm_column_field_select(form.route)
    ) }}
    {{ frm.frm_column(
        frm.frm_column_field(form.ranking),
        frm.frm_column_field(form.icon)
    ) }}
    {{ frm.frm_column(
        frm.frm_column_field(form.badge),
        null,
        null,
        null,
        2
    ) }}
    {{ frm.frm_column(
        frm.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    ) }}
{{ frm.frm_end(form) }}